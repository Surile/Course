.container {
  min-height: 100%;
  position: fixed;
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
}
.imgs{
  width: 750rpx;
  height: 400rpx;
}
image{
  width: 100%;
}
.service{
  height: 48px;
  margin-top: 30px;
  border-radius: 5px;
  line-height: 48px;
  text-align: center;
  width: 80%;
  letter-spacing: 1px;
  font-size: 30rpx; 
  font-family: "PingFangSC-Semibold";
  opacity: 0;
  z-index: 10;
}
.services{
  z-index: 1;
  height: 48px;
  margin-top: -48px;
  border-radius: 5px;
  background-color: #731cc6;
  border: 1px solid #731cc6;
  color: #fff;
  line-height: 48px;
  text-align: center;
  width: 80%;
  letter-spacing: 1px;
  font-size: 30rpx; 
  font-family: "PingFangSC-Semibold";
}
.cell{
  height: 48px;
  margin-top: 30px;
  border-radius: 5px;
  line-height: 48px;
  text-align: center;
  border: 1px solid #eee;
  background-color: #eee;
  width: 80%;
  letter-spacing: 0px;
  font-size: 28rpx; 
  font-family: "PingFangSC-Regular";
}